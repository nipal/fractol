let SessionLoad = 1
if &cp | set nocp | endif
noremap 	 :tabn
noremap  :tabedit   
noremap # :s/^/#	//ljlkjlkj
let s:cpo_save=&cpo
set cpo&vim
noremap '; :s///g<Left><Left><Left>
noremap -o :set smartindent
noremap --o :set nosmartindent
noremap .. :s/^[/#]*	///ljlkjlkj
noremap // :s/^/\/\/	//ljlkjlkj
noremap ;; :%s///g<Left><Left><Left>
noremap ffg /[a-z_]*([a-z]*_[a-z]* [a-z*]*,*
nmap gx <Plug>NetrwBrowseX
noremap ggf /[a-z_]*([a-z]*_[a-z]*[,)]
nmap <F1> :Stdheader
nnoremap <silent> <Plug>NetrwBrowseX :call netrw#NetrwBrowseX(expand("<cWORD>"),0)
noremap <S-Tab> :tabp
